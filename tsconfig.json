{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "ESNext",
    "module": "commonjs",
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "skipLibCheck": true /* Skip type checking all .d.ts files. */
  },
  "include": ["src/**/*"], 
  "exclude": ["src/**/*.spec.ts", "src/**/*.test.ts"]
}
